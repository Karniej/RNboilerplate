{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean && rm -rf node_modules && yarn"
  },
  "dependencies": {
    "react": "16.5.1",
    "react-native": "0.57.1",
    "@netguru-team-m/liquid-design-react-native": "^0.1.23",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-serializer-enzyme": "^1.0.0",
    "lodash": "^4.17.10",
    "patch-package": "^5.1.1",
    "prop-types": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.2",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.11.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "detox": "^9.0.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "^0.45.6",
    "react-test-renderer": "16.5.0",
    "babel-eslint": "^8.2.6",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^23.4.2",
    "postinstall-postinstall": "^1.0.0",
    "prettier-eslint": "^8.8.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/DetoxTesting.app",
        "build": "xcodebuild -project ios/DetoxTesting.xcodeproj -scheme DetoxTesting -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=NO",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
